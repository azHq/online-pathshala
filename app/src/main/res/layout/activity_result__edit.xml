<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:weightSum="10"
    android:orientation="vertical"
    tools:context=".Result_Edit">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_margin="10dp"
        android:layout_weight="9"
        android:layout_height="0dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:background="@drawable/table_border"
            android:gravity="center_horizontal"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Student Id:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/student_id"
                    android:text="Id"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Student Name:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/student_name"
                    android:text="Id"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Subject Name:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/subject_name"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Total Marks:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/total_marks"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:inputType="number"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Subjective Total:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/subjective_total"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Objective Total:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/objective_total"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Practical Total:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/practical_total"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Subjective Obtain:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/subjective_obtain"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Objective Obtain:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/objective_obtain"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Practical Obtain:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/practical_obtain"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:weightSum="2"
                android:layout_height="45dp">
                <TextView
                    android:text="Weight:"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/table_cell"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </TextView>
                <EditText
                    android:id="@+id/weight"
                    android:text="Id"
                    android:textColor="@color/black"
                    android:background="@drawable/table_cell"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:inputType="number"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </EditText>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_weight="1"
        android:layout_margin="10dp"
        android:layout_height="0dp">
        <Button
            android:id="@+id/submit"
            android:background="#0E0977"
            android:text="Submit"
            android:textColor="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </Button>
    </LinearLayout>
</LinearLayout>